<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.*?>
<GridPane alignment="center" hgap="10"
          vgap="10" xmlns:fx="http://javafx.com/fxml/1"
          xmlns="http://javafx.com/javafx/8" fx:controller="openCVProject.MedianFilter.MedianFilterController">
    <columnConstraints>
        <ColumnConstraints/>
    </columnConstraints>
    <rowConstraints>
        <RowConstraints/>
    </rowConstraints>
    <children>
        <VBox GridPane.hgrow="SOMETIMES" GridPane.vgrow="SOMETIMES">
            <children>
                <HBox>
                    <children>
                        <ImageView fx:id="beforeImg" pickOnBounds="true" preserveRatio="true"/>
                        <ImageView fx:id="afterImg" pickOnBounds="true" preserveRatio="true"/>
                    </children>
                </HBox>
                <HBox prefHeight="258.0" prefWidth="955.0"/>
            </children>
        </VBox>
    </children>
</GridPane>
