<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Separator?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.*?>
<GridPane alignment="TOP_CENTER" hgap="10" vgap="10" xmlns="http://javafx.com/javafx/8"
          xmlns:fx="http://javafx.com/fxml/1" fx:controller="openCVProject.MedianFilter.MedianFilterController">
    <columnConstraints>
        <ColumnConstraints/>
    </columnConstraints>
    <rowConstraints>
        <RowConstraints/>
    </rowConstraints>
    <VBox>
        <HBox alignment="CENTER">
            <VBox.margin>
                <Insets/>
            </VBox.margin>
            <Button minHeight="25.0" minWidth="48.0" mnemonicParsing="false"
                    onAction="#returnToPreviousState" prefHeight="25.0" prefWidth="48.0" text="Voltar"
                    HBox.hgrow="NEVER"/>
        </HBox>
        <HBox>
            <VBox fx:id="beforeImgBox">
                <ImageView fx:id="beforeImg" pickOnBounds="true" preserveRatio="true"/>
            </VBox>
            <Separator orientation="VERTICAL" prefHeight="200.0"/>
            <VBox fx:id="afterImgBox">
                <ImageView fx:id="afterImg" pickOnBounds="true" preserveRatio="true"/>
            </VBox>
        </HBox>
        <Separator prefWidth="200.0"/>
        <HBox/>
    </VBox>
</GridPane>
