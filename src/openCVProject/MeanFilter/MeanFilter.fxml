<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.*?>
<GridPane alignment="TOP_CENTER" hgap="10" vgap="10" xmlns="http://javafx.com/javafx/8"
          xmlns:fx="http://javafx.com/fxml/1" fx:controller="openCVProject.MeanFilter.MeanFilterController">
    <columnConstraints>
        <ColumnConstraints percentWidth="50.0"/>
        <ColumnConstraints minWidth="10.0" percentWidth="50.0" prefWidth="100.0"/>
    </columnConstraints>
    <rowConstraints>
        <RowConstraints percentHeight="4.0"/>
        <RowConstraints percentHeight="96.0"/>
    </rowConstraints>
    <Button minHeight="25.0" minWidth="48.0" mnemonicParsing="false" onAction="#returnToPreviousState" prefHeight="25.0"
            prefWidth="48.0" text="Voltar"/>
    <ScrollPane GridPane.rowIndex="1" fx:id="beforeImgScroll">
        <ImageView fx:id="beforeImg" pickOnBounds="true" preserveRatio="true"/>
    </ScrollPane>
    <ScrollPane GridPane.columnIndex="1" GridPane.rowIndex="1" fx:id="afterImgScroll">
        <ImageView fx:id="afterImg" pickOnBounds="true" preserveRatio="true"/>
    </ScrollPane>
</GridPane>
